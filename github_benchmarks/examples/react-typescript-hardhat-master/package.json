  "scripts": {
    "git-push": "git push -f origin",
    "git-pull": "git pull -f origin",
    "add-android": "npx cap add android",
    "add-ios": "npx cap add ios",
    "gen-android-icons": "cordova-res android --skip-config --copy",
    "pre-android-icons": "npm install --ignore-scripts=false --verbose  --force --legacy-peer-deps sharp",
    "gen-ios-icons": "cordova-res ios --skip-config --copy",
    "deploy-android": "adb.exe install -r --install-location 2 C:\\apps\\projects\\project-vongai\\android\\app\\build\\outputs\\apk\\debug\\app-debug.apk",
    "rstart": "react-scripts start",
    "rbuild": "react-scripts build",
    "rtest": "react-scripts test --env=jsdom",
    "reject": "react-scripts eject",
    "rexport": "npm run build-capacitor-android && npx cap sync android && npx cap run android",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
    "build-web": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap sync ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap sync android",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/dovellous/assets/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/dovellous/assets/fonts/",
    "watch": "copy-and-watch --watch ./src/dovellous/**/*.{html,json,png,jpg,svg,ts,tsx,js,jsx,html} ./src/dovellous/**/*. ../project-df7-dist",
    "update-npm": "npm i -g npm-check-updates",
    "update-check": "ncu",
    "update": "ncu -u",
    "cap-migrate": "npx cap migrate",
    "install-all": "npm install --force --legacy-peer-deps",
    "install-cross-env": "npm install --save-dev cross-env --force --legacy-peer-deps",
    "install-capacitor-latest": "npm i -D @capacitor/cli@latest --force --legacy-peer-deps",
    "app-release": "cd android && ./gradlew clean && ./gradlew :app:bundleRelease",
    "app-release-win": "cd android && gradlew clean && gradlew :app:bundleRelease",
    "clean-gradle": "rm -r android/build ; rm -r android/app/src/release/res ; rm -r android/app/build/intermediates ; watchman watch-del-all ; npm start -- --reset-cache",
    "clean-gradle-win": "del -r android/build /F /Q ; del -r android/app/src/release/res /F /Q ; del -r android/app/build/intermediates /F /Q ; watchman watch-del-all ; npm start -- --reset-cache",
    "cache-clean": "npm cache clean --force",
    "clean-cache": "npm run cache-clean",
    "fix-perms": "Set-ExecutionPolicy RemoteSigned -Scope CurrentUser",
    "hcd": "yarn h-compile-deploy",
    "h-compile-deploy": "yarn clean && yarn compile && yarn h-deploy",
    "h-deploy": "npx hardhat run scripts/libs/deploy-setup.js",
    "h-test-bep20": "npx hardhat run scripts/contracts-js/bep20/bep20-tests.js",
    "h-test-erc20": "npx hardhat run scripts/contracts-js/erc20/erc20-test.js",
    "h-test-erc721": "npx hardhat run scripts/contracts-js/erc721/erc721-test.js",
    "h-test-erc1155": "npx hardhat run scripts/contracts-js/erc1151/erc1151-test.js",
    "h-test-custom": "npx hardhat run scripts/contracts-js/custom/custom-test.js",
    "h-flatten": "npx hardhat flatten src/token/BEP20/BEP20SmartContract.sol > contracts/BEP20SmartContract.sol",
    "h-flattenx": "npx hardhat flatten src/token/BEP20/BEP20SmartContract.sol | awk '/SPDX-License-Identifier/&&c++>0 {next} 1' | awk '/pragma experimental ABIEncoderV2;/&&c++>0 {next} 1' > contracts/BEP20SmartContract_h.sol",
    "t-flatten": "truffle-flattener src/token/BEP20/BEP20SmartContract.sol | awk '/SPDX-License-Identifier/&&c++>0 {next} 1' | awk '/pragma experimental ABIEncoderV2;/&&c++>0 {next} 1' > contracts/BEP20SmartContract_t.sol",
    "h-merge": "sol-merger src/token/BEP20/BEP20SmartContract.sol | awk '/SPDX-License-Identifier/&&c++>0 {next} 1' | awk '/pragma experimental ABIEncoderV2;/&&c++>0 {next} 1' contracts/",
    "h-create": "npx hardhat",
    "h-compile": "npx hardhat compile",
    "h-console": "npx hardhat console",
    "h-accounts": "npx hardhat accounts",
    "h-clean": "npx hardhat clean",
    "h-test": "npx hardhat test",
    "h-node": "npx hardhat node",
    "sol-start": "ts-node src/client/main.ts",
    "sol-start-with-test-validator": "start-server-and-test 'solana-test-validator --reset --quiet' http://localhost:8899/health start",
    "sol-lint": "eslint --ext .ts src/client/* && prettier --check \"src/client/**/*.ts\"",
    "sol-lint:fix": "eslint --ext .ts src/client/* --fix && prettier --write \"src/client/**/*.ts\"",
    "sol-clean": "npm run clean:program-c && npm run clean:program-rust",
    "sol-build:program-c": "V=1 make -C ./src/program-c helloworld",
    "sol-clean:program-c": "V=1 make -C ./src/program-c clean",
    "sol-build:program-rust": "cargo build-bpf --manifest-path=./src/program-rust/Cargo.toml --bpf-out-dir=dist/program",
    "sol-clean:program-rust": "cargo clean --manifest-path=./src/program-rust/Cargo.toml && rm -rf ./dist",
    "sol-test:program-rust": "cargo test-bpf --manifest-path=./src/program-rust/Cargo.toml",
    "sol-pretty": "prettier --write '{,src/**/}*.ts'",
    "fix": "yarn fix1 & fix2 & fix3",
    "fix1": "export NODE_OPTIONS=--openssl-legacy-provider",
    "fix2": "Set-ExecutionPolicy -ExecutionPolicy RemoteSigned -Scope LocalMachine",
    "fix3": "react-scripts â€“openssl-legacy-provider start"
  },
